<%= content_tag :section, html_options do %>
  <%= render(Irelia::Container::Component.new(
    size: full_width ? :full_width : :xl,
    center: true,
    html_options: { class: "irelia-navbar__inner" })) do %>
    <% components.each do |component| %>
      <%= component %>
    <% end %>
  <% end %>
<% end %>
